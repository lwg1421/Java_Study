package day1.array;

import java.util.Iterator;

public class TimeCheck {

	public static void main(String[] args) {
		int N = 10000;
		long start, end;
		
		int total = 0;
		
		for (int i = 0; i < N; i++) {
			total += i;
		}
		end = System.currentTimeMillis();
		System.out.println(end-start);
	}

}
