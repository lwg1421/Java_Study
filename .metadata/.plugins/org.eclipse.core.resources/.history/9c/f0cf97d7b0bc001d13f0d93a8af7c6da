package com.kb.cms2;

public class Car {
	String num;
	int price;
	
	public void drive() {
		System.out.println("시속 60km로 달립니다.");
	}
	
	public void drive(int speed) {
		System.out.println("시속 "+speed+"km로 달립니다.");
		System.out.printf("시속 %d km로 달립니다.\n",speed);
	}

	@Override
	public String toString() {
		return "Car [num=" + num + ", price=" + price + "]";
	}
	
	

}
