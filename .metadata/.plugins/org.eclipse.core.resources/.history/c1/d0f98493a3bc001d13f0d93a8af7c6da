package com.kb.cms;

public class Test {

	public static void main(String[] args) {
		Car c = new Car();
		c.num = "서울나1234";
		c.price = 50000000;
		c.drive();
		System.out.println(c);
		
		Bus b = new Bus();
		b.num = "서울나1234";
		b.price = 5000;
		b.seat = 45;
		b.drive();
		System.out.println(b);
	}

}
