package quiz_0315_이원근;

import java.util.Scanner;

// 조합 사용
// 서로 다른 N개의 원소에서 중복을 허용하지 않고 3개를 순서 없이 선택하자.

public class kb1반_알고리즘2번_이원근 {

	public static void main(String[] args) {
		int[] arr = {1,2,3};
		int sum = 0;
		for (int i = 0; i < arr.length; i++) {
			sum += arr[i];
		}
		System.out.println(sum);
		
	}

}
