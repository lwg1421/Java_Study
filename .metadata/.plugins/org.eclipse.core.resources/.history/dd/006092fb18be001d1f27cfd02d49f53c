package day1.array;

import java.util.Iterator;

public class TimeCheck {

	public static void main(String[] args) {
		int N = 10000;
		long start, end;
		
		int total = 0;
		
		start = System.currentTimeMillis();
		for (int i = 0; i < N; i++) {
			total += i;
		}
		end = System.currentTimeMillis();
		System.out.println(end - start);
		
		
		start = System.currentTimeMillis();
		for (int i = 0; i < N; i++) {
			for (int j = 0; j < N; j++) {
				
			}
			total += i;
		}
		end = System.currentTimeMillis();
		System.out.println(end - start);
	}

}
